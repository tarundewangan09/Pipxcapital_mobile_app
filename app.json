{
  "expo": {
    "name": "PipXcapital",
    "slug": "PipXcapital",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/PipXcapital.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/PipXcapital.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true,
          "NSAllowsLocalNetworking": true
        }
      },
      "bundleIdentifier": "com.PipXcapital.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/PipXcapital.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.PipXcapital.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "83e4d15e-0e35-4e9d-b7d7-27997d979bbd"
      }
    },
    "plugins": [
      "expo-font"
    ]
  }
}
